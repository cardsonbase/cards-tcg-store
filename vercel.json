{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://yourdomain.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://yourdomain.com"
        }
      ]
    }
  ]
}
{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install"
}
